<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_notice_claim" model="ir.sequence">
            <field name="name">Secuencia Aviso de Siniestro</field>
            <field name="code">broker.claim.notice</field>
            <field name="prefix">AS/{contract}/{name}/</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_payment_data" model="ir.sequence">
            <field name="name">Secuencia pago de cuotas contrato</field>
            <field name="code">fee.payment</field>
            <field name="prefix">PAGO/</field>
            <field name="padding">5</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_broker_claim_data" model="ir.sequence">
            <field name="name">Secuencia siniestros</field>
            <field name="code">broker.claim</field>
            <field name="prefix">BS/%(year)s- %(month)s-%(day)s/</field>
            <field name="padding">3</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>