<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="broker_object_deductible_template_form" model="ir.ui.view">
        <field name="name">broker.object.deductible.template.form</field>
        <field name="model">broker.object.deductible.template</field>
        <field name="arch" type="xml">
            <form string="Plantilla de deducibles">
                <header>
                </header>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <h1>
                                <label for="name" string="Plantilla de deducible"/>
                            </h1>
                            <field name="name"/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <record id="broker_object_deductible_template_tree" model="ir.ui.view">
        <field name="name">broker.object.deductible.template.tree</field>
        <field name="model">broker.object.deductible.template</field>
        <field name="arch" type="xml">
            <tree string="Plantilla de deducibles">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="broker_object_deductible_template_action" model="ir.actions.act_window">
        <field name="name">Plantilla de deducibles</field>
        <field name="res_model">broker.object.deductible.template</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Ingrese una plantilla de deducibles
            </p>
        </field>
    </record>

</odoo>
